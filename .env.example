# ============================================
# LangGraph Voice Agents - Environment Config
# ============================================

# Voice Provider (Cartesia - NO OpenAI)
CARTESIA_API_KEY=your_cartesia_api_key_here

# LLM Providers (Choose one or more)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here
OPENROUTER_API_KEY=your_openrouter_api_key_here

# LiveKit WebRTC (for real-time audio)
LIVEKIT_URL=wss://your-instance.livekit.cloud
LIVEKIT_API_KEY=your_livekit_api_key_here
LIVEKIT_API_SECRET=your_livekit_api_secret_here

# State Management (optional - defaults shown)
REDIS_URL=redis://localhost:6379/0
DATABASE_URL=sqlite:///agent_states.db

# Default LLM Settings
DEFAULT_LLM_PROVIDER=claude  # Options: claude, gemini, openrouter
DEFAULT_LLM_MODEL=claude-sonnet-4-5-20250929

# Voice Settings
CARTESIA_VOICE_ID=a0e99841-438c-4a64-b679-ae501e7d6091
CARTESIA_SAMPLE_RATE=22050
CARTESIA_LANGUAGE=en

# Logging
LOG_LEVEL=INFO
